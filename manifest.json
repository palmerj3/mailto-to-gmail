{
  "manifest_version": 2,

  "name": "Mailto to GMail Compose",
  "description": "This extension converts mailto links on websites to GMail compose",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "exclude_matches": ["https://mail.google.com/*"],
      "js": ["main.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}